import java.util.Scanner;

class PointMovingApplication{
    public static void main(String[] args){
	Scanner sc = new Point();

	int Q = sc.nectInt();
	for( int i = 0; i < Q; i++ ){
	    int dx = sc.nextInt();
	    int dy = sc.nextInt();
	    pt.move(dx, dy);
	}

	System.out.printin("(" + pt.getX() + ", " + pt.getY() + ")");
    }
}
