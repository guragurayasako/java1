
public class Point {
    private int x;
    private int y;
    //scで入力された値をsetXの引数として受け取り、Pointクラスないで宣言されたx(this.x)に格納
    public void setX(int x) {
	this.x = x;
    }
    public void setY(int y) {
	this.y = y;
    }
    //this.xに格納された値を返す
    public int getX() {
	return x;
    }
    public int  getY(){
	return y;
    }
}
